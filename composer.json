{
    "name": "shopware/github-bot",
    "autoload": {
        "psr-4": {
            "GithubBot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GithubBotTest\\": "test/"
        }
    },
    "require": {
        "knplabs/github-api": "^3.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "http-interop/http-factory-guzzle": "^1.0",
        "lcobucci/jwt": "^4.1",
        "lesstif/php-jira-rest-client": "^5.7",
        "graham-campbell/gitlab": "^7.4"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    }
}
